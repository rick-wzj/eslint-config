{
  "name": "@saremid/eslint-config",
  "version": "0.0.1",
  "description": "Eslint shared config for react with typescript",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "files": [
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rick-wzj/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "react",
    "typescript",
    "prettier"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rick-wzj/eslint-config/issues"
  },
  "homepage": "https://github.com/rick-wzj/eslint-config#readme",
  "peerDependencies": {
    "eslint": ">=8",
    "prettier": ">=2.7",
    "typescript": ">=4.6"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "devDependencies": {
    "eslint": "^8.27.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{ts,js}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
